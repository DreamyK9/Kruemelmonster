{
  "name": "No More Cookies",
  "description": "Banning Cookies on Google.de",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.google.de/*"],
      "js": ["index.js"]
    }
  ],
  "action": {
        "default_popup": "index.html",
        "default_title": "cookie manager"
    }
}
