{
  "name": "Krümelmonster",
  "description": "Our mission is to remove Cookie Banners all across the web",
  "version": "1.3.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["types.js", "actions.js", "rules.js", "index.js"]
    }
  ],
  "action": {
        "default_popup": "popup/index.html",
        "default_title": "Krümelmonster"
    }
}
